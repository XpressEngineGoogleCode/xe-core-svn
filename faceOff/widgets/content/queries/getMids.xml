<query id="getMids" action="select">
    <tables>
        <table name="modules" />
    </tables>
    <columns>
        <column name="site_srl" />
        <column name="module_srl" />
        <column name="mid" />
        <column name="browser_title" />
    </columns>
    <conditions>
        <condition operation="equal" column="site_srl" var="site_srl" />
        <condition operation="in" column="module_srl" var="module_srls" pipe="and" />
    </conditions>
</query>
