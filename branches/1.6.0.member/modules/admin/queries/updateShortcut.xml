<query id="updateShortcut" action="update">
	<tables>
		<table name="admin_shortcuts" />
	</tables>
	<columns>
		<column name="member_srl" var="member_srl" />
		<column name="link" var="link" notnull="notnull" />
		<column name="display_name" var="display_name" notnull="notnull" />
		<column name="order_number" var="order_number" notnull="notnull" />
	</columns>
	<conditions>
		<condition operation="equal" column="shortcut_srl" var="shortcut_srl" filter="number" notnull="notnull" />
	</conditions>
</query>